{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Socrates quotes",
    "description": "workshop of Rawan",
    "chrome_url_overrides": {
        "newtab": "new_tab.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "https://homwwork2.herokuapp.com/*"
    ],
    "action": {
        "default_icon": {
            "16": "/images/get_started16.png",
            "32": "/images/get_started32.png",
            "48": "/images/get_started48.png",
            "128": "/images/get_started128.png"
        }
    },
    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"        
    }
}